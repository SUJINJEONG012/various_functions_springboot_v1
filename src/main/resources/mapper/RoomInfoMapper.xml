<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.various_functions.admin.mapper.RoomInfoMapper">

	<!-- 객실 정보 삽입 -->
    <insert id="insertRoomInfo" 
    parameterType="com.various_functions.admin.dto.RoomInfoDto"
    useGeneratedKeys="true" keyProperty="riid">
       
        <!-- 객실 정보를 삽입하고 생성된 키를 가져옴 -->      
        INSERT INTO room_info (
        riroomtype, 
        riroom, 
        riservice, 
        risize, 
        riminper, 
        rimaxper, 
        ripeak, 
        risemipeak, 
        rioff,
        aid
        ) 
        VALUES (
        #{riroomtype}, 
        #{riroom}, 
        #{riservice}, 
        #{risize}, 
        #{riminper}, 
        #{rimaxper}, 
        #{ripeak}, 
        #{risemipeak}, 
        #{rioff},
        #{accommodations.aid}
        )
    </insert>
    


</mapper>